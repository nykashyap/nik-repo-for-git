def func(*proglang):
    print("Programming Language known: ")
    for x in proglang:
        print(x)



print("1st function call")


print("\n2st function call")
func("C","C++","java","python")   


print("\n3st function call")
func("C","C++","kotlin","javascript")   

def capitals(country,capital):
    print("country:",country,">> capital:",capital)

capitals("India","New Delhi")

capitals("UsA","Washington DC")
